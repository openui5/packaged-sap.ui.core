{
	"_version" : "1.1.0",
	"sap.app" : {
		"_version" : "1.1.0",
		"ach" : "CA-UI5-COR",
		"applicationVersion" : {
			"version" : "${version}"
		},
		"description" : "{{sap.app.description}}",
		"dataSources" : {
			"default" : {
				"uri" : "/sap/opu/odata4/sap/zui5_testv4/default/sap/zui5_epm_sample/0001/?custom-option=value",
				"type" : "OData",
				"settings" : {
					"odataVersion" : "4.0"
				}
			}
		},
		"id" : "sap.ui.core.sample.odata.v4.SalesOrders",
		"i18n" : "i18n/i18n.properties",
		"title" : "{{sap.app.title}}",
		"type" : "application"
	},
	"sap.ui" : {
		"_version" : "1.1.0",
		"deviceTypes" : {
			"desktop" : true,
			"phone" : true,
			"tablet" : true
		},
		"supportedThemes" : [
			"sap_belize",
			"sap_hcb"
		],
		"technology" : "UI5"
	},
	"sap.ui5" : {
		"_version" : "1.1.0",
		"config" : {
			"sample" : {
				"files" : [
					"Component.js",
					"Main.controller.js",
					"Main.view.xml",
					"../../../common/ValueHelp.js"
				]
			}
		},
		"contentDensities" : {
			"compact" : true,
			"cozy" : true
		},
		"dependencies" : {
			"minUI5Version" : "1.37",
			"libs" : {
				"sap.m" : {}
			}
		},
		"handleValidation" : true,
		"models" : {
			"" : {
				"dataSource" : "default",
				"settings" : {
					"autoExpandSelect" : true,
					"operationMode" : "Server",
					"synchronizationMode" : "None",
					"updateGroupId" : "UpdateGroup"
				}
			}
		},
		"rootView" : "Main.view.xml"
	}
}
