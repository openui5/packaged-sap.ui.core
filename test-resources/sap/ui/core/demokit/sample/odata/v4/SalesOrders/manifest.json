{
	"_version" : "1.8.0",
	"sap.app" : {
		"ach" : "CA-UI5-COR",
		"applicationVersion" : {
			"version" : "${version}"
		},
		"description" : "{{sap.app.description}}",
		"dataSources" : {
			"default" : {
				"uri" : "/sap/opu/odata4/sap/zui5_testv4/default/sap/zui5_epm_sample/0002/?custom-option=value",
				"type" : "OData",
				"settings" : {
					"odataVersion" : "4.0"
				}
			}
		},
		"id" : "sap.ui.core.sample.odata.v4.SalesOrders",
		"i18n" : "i18n/i18n.properties",
		"title" : "{{sap.app.title}}",
		"type" : "application"
	},
	"sap.ui" : {
		"deviceTypes" : {
			"desktop" : true,
			"phone" : true,
			"tablet" : true
		},
		"technology" : "UI5"
	},
	"sap.ui5" : {
		"config" : {
			"sample" : {
				"files" : [
					"Component.js",
					"Main.controller.js",
					"Main.view.xml",
					"manifest.json",
					"../../../common/ValueHelp.js"
				]
			}
		},
		"contentDensities" : {
			"compact" : true,
			"cozy" : true
		},
		"dependencies" : {
			"minUI5Version" : "1.37",
			"libs" : {
				"sap.m" : {}
			}
		},
		"handleValidation" : true,
		"models" : {
			"" : {
				"dataSource" : "default",
				"settings" : {
					"autoExpandSelect" : true,
					"earlyRequests": true,
					"groupProperties" : {
						"FavoriteProductGroup" : {"submit" : "Auto"},
						"myAutoGroup" : {"submit" : "Auto"},
						"myDirectGroup" : {"submit" : "Direct"}
					},
					"operationMode" : "Server",
					"synchronizationMode" : "None",
					"updateGroupId" : "UpdateGroup"
				}
			}
		},
		"rootView" : {
			"async" : true,
			"type" : "XML",
			"viewName" : "Main.view.xml"
		},
		"resources": {
			"js": [{
				"uri": "SalesOrdersSandbox.js"
			}]
		}
	}
}
