{
	"_version" : "1.8.0",
	"sap.app" : {
		"ach" : "CA-UI5-COR",
		"applicationVersion" : {
			"version" : "${version}"
		},
		"description" : "{{sap.app.description}}",
		"dataSources" : {
			"default" : {
				"uri" : "/sap/opu/odata4/sap/zui5_testv4/default/sap/zui5_epm_sample/0002/",
				"type" : "OData",
				"settings" : {
					"annotations": ["testanno"],
					"odataVersion" : "4.0"
				}
			},
			"testanno": {
				"uri": "data/annotations.xml",
				"type": "ODataAnnotation"
			}
		},
		"id" : "sap.ui.core.sample.odata.v4.SalesOrdersTemplate",
		"i18n" : "i18n/i18n.properties",
		"title" : "{{sap.app.title}}",
		"type" : "application"
	},
	"sap.ui" : {
		"deviceTypes" : {
			"desktop" : true,
			"phone" : true,
			"tablet" : true
		},
		"technology" : "UI5"
	},
	"sap.ui5" : {
		"config" : {
			"sample" : {
				"files" : [
					"Component.js",
					"FormattedText.fragment.xml",
					"Helper.js",
					"Main.view.xml",
					"../../../common/ValueHelp.js"
				]
			}
		},
		"contentDensities" : {
			"compact" : true,
			"cozy" : true
		},
		"dependencies" : {
			"minUI5Version" : "1.37",
			"libs" : {
				"sap.m" : {}
			}
		},
		"handleValidation" : false,
		"models" : {
			"" : {
				"dataSource" : "default",
				"settings" : {
					"synchronizationMode" : "None"
				}
			}
		},
		"rootView" : {
			"async" : true,
			"type" : "XML",
			"viewName" : "Main.view.xml"
		}
	}
}